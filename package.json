{
  "name": "hanmat-fileserver",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "build": "pkg ."
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.4.7",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "mongoose": "^8.9.3",
    "morgan": "~1.9.1",
    "pug": "2.0.0-beta11",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  },
  "bin": {
    "app": "./bin/www"
  },
  "pkg": {
    "targets": [
      "node*-linux-x64",
      "node*-win-x64"
    ],
    "assets": [
      "public/**/*",
      "views/**/*"
    ],
    "outputPath": "./dist"
  },
  "main": "app.js"
}
